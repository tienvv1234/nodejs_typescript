{
  "name": "test1",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "10.15.0"
  },
  "scripts": {
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "mocha --opts ./src/tests/mocha.opts --require ts-node/register src/tests/unit/**/*.spec.ts",
    "test:integration": "export NODE_ENV=test&& mocha --opts ./src/tests/mocha.opts --require ts-node/register src/tests/integration/**/*.spec.ts --exit",
    "build": "npm run clean && webpack -p --config ./webpack.config.js --progress",
    "build:tsc": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json --watch",
    "heroku-postbuild": "npm run build:tsc",
    "dev": "node ./dist/bundle.js",
    "start": "node dist/app.js",
    "start:tsc": "nodemon ./dist/app.js",
    "migrate:production": "knex migrate:latest --knexfile src/knexfile.js",
    "seed:production": "knex seed:run --knexfile src/knexfile.js",
    "clean": "rimraf dist",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json",
    "debug": "node --inspect-brk --trace-warnings ./dist/app.js",
    "start:debug": "concurrently \"npm run build:watch\" \"npm run debug\"",
    "start:dev": "concurrently \"npm run build:watch\" \"npm run start:tsc\"",
    "test-coverage": "nyc --reporter=text --check-coverage npm run test"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "class-validator": "^0.9.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "helmet": "^3.18.0",
    "json-typescript-mapper": "^1.1.3",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "nyc": "^14.1.1",
    "pg": "^7.11.0",
    "redis": "^2.8.0",
    "winston": "^3.2.1",
    "knex": "^0.16.5",
    "objection": "^1.6.8"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/faker": "^4.1.5",
    "@types/helmet": "0.0.43",
    "@types/knex": "^0.16.1",
    "@types/mocha": "^5.2.6",
    "@types/morgan": "^1.7.35",
    "@types/node": "^12.0.2",
    "@types/redis": "^2.8.12",
    "@types/sinon": "^7.0.11",
    "@types/sinon-chai": "^3.2.2",
    "@types/supertest": "^2.0.7",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "concurrently": "^4.1.0",
    "faker": "^4.1.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.0",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "source-map-support": "^0.5.12",
    "supertest": "^4.0.2",
    "ts-loader": "^6.0.0",
